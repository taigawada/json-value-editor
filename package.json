    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.69.4",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "typescript-plugin-css-modules": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.0",
    "vite-plugin-lib-inject-css": "^1.3.0"
